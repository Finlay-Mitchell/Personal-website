            <error statusCode="401" prefixLanguageFilePath="" path="http://ec2-35-176-187-24.eu-west-2.compute.amazonaws.com/Error%20pages/401-unauthorised.html" responseMode="Redirect" />
            <error statusCode="500" prefixLanguageFilePath="" path="http://ec2-35-176-187-24.eu-west-2.compute.amazonaws.com/error%20pages/500-internal-error.html" responseMode="Redirect" />
            <error statusCode="502" prefixLanguageFilePath="" path="http://ec2-35-176-187-24.eu-west-2.compute.amazonaws.com/error%20pages/502-bad-gateway.html" responseMode="Redirect" />
        </httpErrors>
        <directoryBrowse enabled="false" />
